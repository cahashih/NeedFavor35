
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Clickable List View</title>
</head>
<body>
    <h2>Users</h2>
    <ul>
        @foreach (var user in ViewBag.Users)
        {
            <li>@user.Title</li>
            <li>@user.Description</li>
            <li>@user.HaveCostStart</li>
            <li>@user.HaveCost</li>
        }
    </ul>

</body>
</html>